{
    "manifest_version": 3,
    "name": "Copiare",
    "version": "1.0",
    "description": "Copy selected text from any webpage",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }